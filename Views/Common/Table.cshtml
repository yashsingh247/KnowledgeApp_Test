
@{
    Layout = null;
}

<!DOCTYPE html>

<<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Table</title>
    <link href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
    <script>


        $(document).ready(function () {
                  GetEmployeeData()

        });
        function GetEmployeeData()
        {
            $('#enterprise').DataTable({
                "ajax": {
                    "url": "/common/EnterpriseData",
                    "type": "Post",
                    "datatype": "json"
                },
                "columns": [{ "data": "EnterpriseId" },
                { "data": "EnterpriseName" },
                { "data": "AddressLine1" },
                { "data": "AddressLine2" },
                ],
                "serverSide": "true",
                "order": [0, "asc"]
            });
        }
    </script>
</head>
<body>
    <div class="row">
        <div class="col-md-12">
            <div class="table-responsive">
                <table class="table table-bordered" id="enterprise" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th class="EnterpriseId1">EnterpriseId</th>

                            <th class="EnterpriseName">EnterpriseName</th>

                            <th class="AddressLine1">AddressLine1</th>

                            <th class="AddressLine2">AddressLine2</th>
                        </tr>
                    </thead>

                    @*<tbody>
                            <tr>
                                <td>Yash</td>
                                <td>Yash</td>
                                <td>Yash</td>
                                <td>Yash</td>
                            </tr>
                            <tr>
                                <td>Yash</td>
                                <td>Yash</td>
                                <td>Yash</td>
                                <td>Yash</td>
                            </tr>
                        </tbody>*@
                </table>
            </div>

        </div>
    </div>

</body>

</html>
