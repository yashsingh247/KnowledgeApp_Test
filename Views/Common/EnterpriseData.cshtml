@model IEnumerable<KnowledgeApp_Test.Models.Enterprise>

<table id="Enterprise" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
    <thead>
        <tr class="bg-light">
            @*<th></th>*@
            <th class="EnterpriseId1">Enterprise Id</th>
            <th class="EnterpriseName">Enterprise Name</th>
            <th class="AddressLine1">Address Line1</th>
            <th class="AddressLine2">Address Line2</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var i in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => i.EnterpriseId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => i.EnterpriseName)
                </td>

                <td>@Html.DisplayFor(modelItem => i.AddressLine1) </td>
                <td>
                    @Html.DisplayFor(modelItem => i.AddressLine2)
                </td>

            </tr>
        }
    </tbody>

</table>
            
<script>
    $(document).ready(function () {
        var Id ="Enterprise"
        init(Id)
        //$("#Enterprise").DataTable();
       // $('.Table').addClass('bs-select');
    });
</script>
        
